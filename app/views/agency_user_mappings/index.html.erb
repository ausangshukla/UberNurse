<p id="notice"><%= notice %></p>

<h1>Agency User Mappings</h1>

<table>
  <thead>
    <tr>
      <th>Agency</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agency_user_mappings.each do |agency_user_mapping| %>
      <tr>
        <td><%= agency_user_mapping.agency %></td>
        <td><%= agency_user_mapping.user %></td>
        <td><%= link_to 'Show', agency_user_mapping %></td>
        <td><%= link_to 'Edit', edit_agency_user_mapping_path(agency_user_mapping) %></td>
        <td><%= link_to 'Destroy', agency_user_mapping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agency User Mapping', new_agency_user_mapping_path %>
