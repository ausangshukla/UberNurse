<table>
<%
	count = 0 
	field.to_s.each do |row| 
%>
	<% if count == 0 %>
	<tr>
		<% row.attributes.keys.each do |k| %>		
			<th><%= k.titlecase %></th>
		<% end %>
	</tr>
	<% end %>

	<tr>
		<% row.attributes.keys.each do |k| %>		
		<td><%= row[k] %></td>
		<% end %>
	</tr>
<% 
count += 1
end 
%>
</table>
